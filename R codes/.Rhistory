consolidate_data_Frames <- function(dfs, labels_dfs) {
n <- length(dfs)
datos <- dfs[1]
datos$Exp <- as.character(labels_dfs[1])
rango <- seq(2,n)
for (x in rango) {
d <- dfs[x]
d$Exp <- as.character(labels_dfs[x])
rbind(datos, d)
}
return (datos)
}
